])

# Calculate Euclidean distances between each pair of vectors
n = len(vectors)
distances = np.zeros((n, n))
for i in range(n):
    for j in range(i+1, n):
        distance = np.linalg.norm(vectors[i] - vectors[j])
        distances[i, j] = distances[j, i] = distance

# Print the distance matrix
print("Euclidean distances between pairs of vectors:")
print(distances)

# Use MDS to project n-dimensional vectors to 2D space
mds = MDS(n_components=2, dissimilarity="precomputed", random_state=42)
vectors_2d = mds.fit_transform(distances)

# Plot the points in 2D space
plt.figure(figsize=(8, 6))
for i, vector in enumerate(vectors_2d):
    plt.scatter(vector[0], vector[1], label=f'Point {i+1}')
    plt.text(vector[0], vector[1], f'{i+1}')
plt.title('2D Visualization of 6 Points Using MDS')
plt.xlabel('Dimension 1')
plt.ylabel('Dimension 2')
plt.legend()
plt.grid(True)
plt.show()